<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wheeltracker">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/wheeltracker1"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/wheeltracker1"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ConnectToMapActivity">
            <intent-filter>
                <action android:name="com.example.wheeltracker.ConnectToMapActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>


        </activity>
        <activity android:name=".GoogleSignUp">
            <intent-filter>
                <action android:name="com.example.wheeltracker.GoogleSignUp" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ConnectActivity"
            android:theme="@style/AppTheme2">
            <intent-filter>
                <action android:name="com.example.wheeltracker.ConnectActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".WelcomeScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" android:noHistory="true">
            <intent-filter>
                <action android:name="com.example.wheeltracker.MainActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUpActivity">
            <intent-filter>
                <action android:name="com.example.wheeltracker.SignUpActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>